{
  "name": "nodejs-ts-clean-architecture",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "ts-node-dev --poll --respawn --transpile-only --no-deps --exit-child ./src/app.ts",
    "lint": "",
    "clean": "gts clean",
    "compile": "tsc",
    "fix": "gts fix",
    "test": "mocha --require ts-node/register src/test/**/*.test.ts --recursive",
    "test_dist": "mocha dist/test --recursive",
    "coverage": "nyc npm run test",
    "coverage_dist": "nyc npm run test_dist",
    "pretest": "npm.cmd run compile",
    "posttest": "npm.cmd run lint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "dependencies": {
    "@cloudnative/health-connect": "^2.1.0",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "express": "^4.19.2",
    "helmet": "^7.1.0",
    "http-status-codes": "^2.3.0",
    "joi": "^17.13.0",
    "morgan": "^1.10.0",
    "pg": "^8.11.5",
    "pg-hstore": "^2.3.4",
    "reflect-metadata": "^0.1.14",
    "sequelize": "^6.37.3",
    "sequelize-typescript": "^2.1.6",
    "umzug": "^3.8.0",
    "winston": "^3.13.0"
  },
  "devDependencies": {
    "@types/chai": "^4.3.14",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/mocha": "^10.0.6",
    "@types/morgan": "^1.9.9",
    "@types/node": "^14.11.2",
    "@types/sinon": "^10.0.20",
    "@types/validator": "^13.11.9",
    "chai": "^4.4.1",
    "gts": "^3.1.1",
    "mocha": "^10.4.0",
    "nyc": "^15.1.0",
    "sinon": "^15.2.0",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "~4.7.0"
  }
}
