{
  "extends": "./node_modules/gts/",
  "rules": {
    "node/no-unpublished-import": [
      "error", 
      {
        "allowModules": ["sinon", "mocha", "chai"]
      }
    ],
    "prettier/prettier": [
      "error",
      {
        "endOfLine": "auto",
        "bracketSpacing": true,
        "singleQuote": true
      }
    ]
  }
}
